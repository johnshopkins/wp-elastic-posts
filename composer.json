{
  "name": "johnshopkins/wp-elastic-posts",
  "version": "0.1.1",
  "type": "wordpress-plugin",
  "description": "Imports fields of study into elasticsearch.",
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:johnshopkins/wp-utilities.git"
    },
    {
      "type": "vcs",
      "url": "git@github.com:johnshopkins/secrets.git"
    }
  ],
  "require": {
    "elasticsearch/elasticsearch": "1.*",
    "composer/installers": "~1.0",
    "johnshopkins/wp-utilities": "dev-master",
    "johnshopkins/http-exchange": "0.3.*",
    "resty/resty": "0.6.*",
    "johnshopkins/secrets": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.0.*"
  },
  "autoload": {
    "psr-4": {
      "ElasticPosts\\": [
        "src/",
        "tests/"
      ]
    }
  }
}
